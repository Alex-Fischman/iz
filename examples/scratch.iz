#include "std.iz"

divisors := i -> if i == 0 2 else i * 2 + 1
factors := n -> if n <= 1 [] else {
	test := i -> {
		factor := divisors@i
		if n % factor == 0 factor::factors@(n / factor)
		else if factor > n^(1/2) [n]
		else test@(i + 1)
	}
	test@0
}

total := 1

range@1@20.map@factors.map@(l -> {
	running := total

	l.map@(n -> if running % n != 0 {
		total = total * n
	} else {
		running = running / n
	})
})

print total
print factors@total
