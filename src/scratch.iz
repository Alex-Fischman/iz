#include "src/std.iz"

fizzbuzz = max -> n -> {
	print if n % 15 == 0 "fizzbuzz"
	 else if n % 3  == 0 "fizz"
	 else if n % 5  == 0 "buzz"
	 else                n
	if n < max fizzbuzz@max@(n + 1)
}

sum = l -> if (x::xs = l) x + xs.sum else 0
filter = f -> l -> if (x::xs = l) {
	rest = xs.filter@f
	if f@x x::rest else rest
} else []

print [1 2 3 4].map@(x -> x * x).filter@(x -> x % 2 != 0).sum
